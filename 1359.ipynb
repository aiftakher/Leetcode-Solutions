    "import math\n",
    "\n",
    "class Solution:\n",
    "    def countOrders(self, n: int) -> int:\n",
    "        MOD = 10**9 + 7\n",
    "        ans = 1\n",
    "        ans = (math.factorial(2*n) // (2**n)) % MOD\n",
    "        return ans\n",
    "        \n",
    "sol = Solution()\n",
    "n = 3\n",
    "print(sol.countOrders(n))"
   ]
